{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "WorkFlowy MultiFlow",
  "description" : "Multi-column view for WorkFlowy",
  "version": "1.5.1",
  "manifest_version": 2,
  "key": "",
  "icons": {
    "16": "assets/icons/icon-16.png",
    "32": "assets/icons/icon-32.png",
    "48": "assets/icons/icon-48.png",
    "128": "assets/icons/icon-128.png"
  },
  "permissions": [
    "https://workflowy.com/*",
    "https://*.workflowy.com/*",
    "declarativeNetRequest",
    "declarativeContent",
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background/background.js"
    ]
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "web_accessible_resources": [
    "https://workflowy.com/*",
    "https://*.workflowy.com/*",
    "assets/icons/*",
    "content/content.html",
    "content/content.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://workflowy.com/*",
        "https://*.workflowy.com/*"
      ],
      "all_frames": false,
      "run_at": "document_idle",
      "js": [
        "content/content.js"
      ],
      "css": [
        "content/content.css"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_title": "WorkFlowy MultiFlow",
    "default_icon": {
      "32": "assets/icons/icon-16.png"
    }
  },
  "content_security_policy": "default-src chrome-extension://khjdmjcmpolknpccmaaipmidphjokhdf/*; script-src 'self' 'unsafe-eval'; object-src 'self'; img-src * 'self' data: https: http: chrome: chrome://favicon/"
}
